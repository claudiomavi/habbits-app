{
	"expo": {
		"name": "habbits-app",
		"slug": "habbits-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"scheme": "habbitsapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false
		},
		"web": {
			"output": "static",
			"bundler": "metro"
		},
		"plugins": [
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff",
					"dark": {
						"backgroundColor": "#000000"
					}
				}
			],
			"expo-font"
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		}
	}
}
